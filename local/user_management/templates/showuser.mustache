<h1>Show users page from mustache</h1>
<div class="card mt-2 mb-2">
    <div class="card-body">
        <input type="button" class="btn btn-primary" value="create user page"
               onclick="location.href='{{createuserurl}}'">
    </div>
</div>
<hr>
{{#users}}
    <div class="card mt-2 mb-2">
        <div class="card-body">
            <p>users :{{id}}</p>
            <p>user firstname : {{firstname}}</p>
            <p>user lastname : {{lastname}}</p>
            <input type="button" class="btn btn-primary" value="Edit user"
                   onclick="location.href='{{edituserurl}}'">
        </div>
    </div>
{{/users}}